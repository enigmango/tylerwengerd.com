{{!< default}}

	{{#foreach posts}}

        {{#if @last}}
			<div class="twelve columns black">
				<h1 class="headline-front white last-post"><a href="{{url}}">{{{title}}}</a></h1>
			</div>
        {{else}}
            <div class="twelve columns black">
				<h1 class="headline-front white"><a href="{{url}}">{{{title}}}</a></h1>
			</div>
        {{/if}}

	{{/foreach}}
	
    {{pagination}}